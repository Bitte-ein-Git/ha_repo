{
  "name": "Portainer",
  "version": "2.20.2",
  "slug": "portainer",
  "description": "Portainer: web-gui for your Docker environment!",
  "url": "https://github.com/bitte-ein-git/ha_repo/",
  "advanced": true,
  "ingress": true,
  "ingress_port": 9000,
  "ingress_stream": true,
  "startup": "services",
  "panel_icon": "mdi:docker",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
  "init": false,
  "audio": true,
  "ports": {
    "8000/tcp": null
  },
  "ports_description": {
    "8000/tcp": "Edge Agent Api (Enable when managing remote edge agents)"
  },
  "hassio_api": true,
  "docker_api": true,
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "agent_secret": "str?"
  }
}
